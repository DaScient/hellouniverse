    "# A representative HTML snippet for the Audiophile page\n",
    "html_code = '''\n",
    "<!DOCTYPE html>\n",
    "<html lang=\"en\">\n",
    "<head>\n",
    "  <meta charset=\"UTF-8\" />\n",
    "  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n",
    "  <title>Audiophile - High Fidelity Audio | DONUTZ.AI</title>\n",
    "  <!-- External stylesheet for Audiophile-specific styling -->\n",
    "  <link rel=\"stylesheet\" href=\"styles/audiophile.css\" />\n",
    "  <!-- Google Fonts and Icon libraries -->\n",
    "  <link href=\"https://fonts.googleapis.com/css?family=Roboto:400,500,700\" rel=\"stylesheet\" />\n",
    "  <!-- JavaScript file for interactive media effects -->\n",
    "  <script src=\"scripts/audiophile.js\"></script>\n",
    "</head>\n",
    "<body>\n",
    "  <!-- Hero Section with media background -->\n",
    "  <header class=\"hero\">\n",
    "    <h1>Audiophile</h1>\n",
    "    <p>Experience Sound in its Purest Form</p>\n",
    "  </header>\n",
    "  \n",
    "  <!-- Features Section -->\n",
    "  <section id=\"features\">\n",
    "    <div class=\"feature\">\n",
    "      <h2>Immersive Audio</h2>\n",
    "      <p>High‑fidelity sound engineered for the true connoisseur.</p>\n",
    "    </div>\n",
    "    <div class=\"feature\">\n",
    "      <h2>Cutting‑Edge Technology</h2>\n",
    "      <p>State‑of‑the‑art hardware and software integrations for an unparalleled listening experience.</p>\n",
    "    </div>\n",
    "  </section>\n",
    "  \n",
    "  <!-- Footer Section -->\n",
    "  <footer>\n",
    "    <p>&copy; 2025 DONUTZ.AI. All rights reserved.</p>\n",
    "  </footer>\n",
    "  \n",
    "  <!-- Inline JavaScript to initialize interactive audio effects -->\n",
    "  <script>\n",
    "    document.addEventListener('DOMContentLoaded', function() {\n",
    "      console.log('Audiophile page loaded. Initializing audio visualizations...');\n",
    "      // Additional interactive audio code goes here\n",
    "    });\n",
    "  </script>\n",
    "</body>\n",
    "</html>\n",
    "'''"
